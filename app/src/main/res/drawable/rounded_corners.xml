<?xml version="1.0" encoding="UTF-8"?>

<!-- Shape applied as background to keypad buttons to give rounded corners effect. -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#e18d939b" />
    <padding
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp" />
    <corners android:radius="10dp" />
</shape>